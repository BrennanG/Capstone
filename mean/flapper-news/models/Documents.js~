var mongoose = require('mongoose');

// When we update a graph, we don't want to change the document, except by adding the new graph to the array of graphs

var DocumentSchema = new mongoose.Schema({
  title: String,
  graphs: [{type: mongoose.Schema.Types.ObjectId, ref: 'Graph'}]//,
  //users: [{ type: mongoose.Schema.Types.ObjectId, ref: 'User' }]
});

mongoose.model('Document', DocumentSchema);
